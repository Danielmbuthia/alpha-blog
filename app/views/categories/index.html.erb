<h1 align="center">Categories Listing</h1>
<% @categories.each do |category| %>
    <ul class="listing">
       <div class="row">
          <div class="card col-md-4 offset-4">
            <li class="article-title">
               <%= link_to "#{category.name}",category_path(category)%>
            </li>
          </div>
       </div>
    </ul>
<% end %>
<%= will_paginate @categories%>
